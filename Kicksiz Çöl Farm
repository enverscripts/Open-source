loadstring(game:HttpGet("https://raw.githubusercontent.com/KaanFriztche/Open-source-/refs/heads/main/%C3%87%C3%B6lFarm"))()

local a=game.Players.LocalPlayer
local b=a.Character or a.CharacterAdded:Wait()
local function c(d)
    for _,e in ipairs(d:GetDescendants())do
        if e:IsA("BasePart")then
            e.Transparency=1
            e.CanCollide=false
        elseif e:IsA("Decal")then
            e.Transparency=1
        elseif e:IsA("ParticleEmitter")or e:IsA("Trail")then
            e.Enabled=false
        end
    end
    local f=d:FindFirstChild("Humanoid")
    if f then
        f.NameDisplayDistance=0
        f.HealthDisplayDistance=0
    end
end
local g=getrawmetatable(game)
local h=g.__namecall
local i=g.__index
setreadonly(g,false)
g.__namecall=newcclosure(function(j,...)
    local k=getnamecallmethod()
    if(k=="Kick"or k=="Destroy")and j==a then return wait(1e10)end
    return h(j,...)
end)
g.__index=newcclosure(function(j,l)
    if l=="Kick"and j==a then return function()return wait(1e10)end end
    return i(j,l)
end)
setreadonly(g,true)
c(b)
a.CharacterAdded:Connect(function(m)
    b=m
    c(b)
end)