local remoteName = "RewardEvent"
local rewardRemote = nil
for _, v in ipairs(game:GetDescendants()) do
    if v:IsA("RemoteEvent") and v.Name == remoteName then
        rewardRemote = v
        break
    end
end
if rewardRemote then
    while true do
        pcall(function()
            rewardRemote:FireServer()
        end)
    end
end